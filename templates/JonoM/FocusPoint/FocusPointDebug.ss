<div class="focuspoint-field focuspoint-field--debug">
	<div class="focuspoint-field__picker focuspoint-picker">
		<img src="$URL" alt="" width="$Width" height="$Height" class="focuspoint-picker__image" />
		<span class="focuspoint-picker__gradient" style="left:$FocusPoint.PercentageX%; top:$FocusPoint.PercentageY%;"></span>
	</div>
	<p class="focuspoint-field__info">$Name<br>FocusX: $FocusPoint.X, FocusY: $FocusPoint.Y, Class: $ClassName</p>
</div>
