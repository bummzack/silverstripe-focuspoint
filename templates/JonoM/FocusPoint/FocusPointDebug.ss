<div class="focuspoint-debug">
	<div class="focuspoint-image">
		<img src="$URL" alt="" width="$Width" height="$Height" />
		<div class="crosshair" style="left:$FocusPoint.PercentageX%; top:$FocusPoint.PercentageY%;"></div>
	</div>
	<p class="info">$Name<br>FocusX: $FocusPoint.X, FocusY: $FocusPoint.Y, Class: $ClassName</p>
</div>
